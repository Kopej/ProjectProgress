// Define a function to calculate BSI
function calculateBSI(image) {
  var b4 = image.select('B4'); // Red band
  var b5 = image.select('B5'); // Near Infrared (NIR) band
  var b6 = image.select('B6'); // Shortwave Infrared 1 (SWIR1) band
  
  // Calculate BSI
  var bsi = b6.subtract(b4).divide(b6.add(b4)).rename('BSI');
  
  return image.addBands(bsi);
};

